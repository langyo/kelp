    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.3.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "mdi-material-ui": "^6.2.0",
    "portscanner": "^2.2.0",
